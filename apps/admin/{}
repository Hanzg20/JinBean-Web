  await props.params;

  return {
    title: 'ç”¨æˆ·ç®¡ç† - é‡‘è±†èš JinBean',
    description: 'ç®¡ç†é‡‘è±†èšå¹³å°çš„ç”¨æˆ·è´¦æˆ·',
  };
}

